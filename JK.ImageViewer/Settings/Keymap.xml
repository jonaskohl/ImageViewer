<?xml version="1.0" encoding="utf-8" ?>
<Keymap>
    <Shortcut Command="OpenFile">
        <Ctrl />
        <Key Which="O" />
    </Shortcut>
    <Shortcut Command="CloseImage">
        <Ctrl />
        <Key Which="W" />
    </Shortcut>
    <Shortcut Command="ExitApplication">
        <Ctrl />
        <Key Which="Q" />
    </Shortcut>
    <Shortcut Command="ZoomIn">
        <Ctrl />
        <Key Which="Oemplus" />
    </Shortcut>
    <Shortcut Command="ZoomOut">
        <Ctrl />
        <Key Which="OemMinus" />
    </Shortcut>
    <Shortcut Command="ZoomOriginal">
        <Ctrl />
        <Key Which="D0" />
    </Shortcut>
    <Shortcut Command="ZoomToFit">
        <Ctrl />
        <Key Which="D1" />
    </Shortcut>
    <Shortcut Command="FolderImagePrevious">
        <Key Which="Left" />
    </Shortcut>
    <Shortcut Command="FolderImageNext">
        <Key Which="Right" />
    </Shortcut>
    <Shortcut Command="ShowSettings">
        <Ctrl />
        <Key Which="Oemcomma" />
    </Shortcut>
    <Shortcut Command="SaveFile">
        <Ctrl />
        <Key Which="S" />
    </Shortcut>
    <Shortcut Tool="Default">
        <Key Which="V" />
    </Shortcut>
    <Shortcut Tool="Crop">
        <Key Which="C" />
    </Shortcut>
    <Shortcut Tool="Zoom">
        <Key Which="Z" />
    </Shortcut>
    <Shortcut Tool="Rectangle">
        <Key Which="R" />
    </Shortcut>
    <Shortcut Tool="Ellipse">
        <Shift />
        <Key Which="R" />
    </Shortcut>
    <Shortcut Tool="Line">
        <Key Which="L" />
    </Shortcut>
</Keymap>
